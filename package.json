{
  "name": "yasp",
  "description": "YASP: Another (Dota) Stats Page",
  "version": "3.1.0",
  "main": "index.js",
  "scripts": {
    "start": "nf start",
    "dev": "nf -j Procfile.dev start",
    "lint": "eslint public",
    "test": "npm run lint && rm -rf testfiles && git clone https://github.com/yasp-dota/testfiles && npm run build && istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "update": "npm-check-updates -u -f '/^((?!JSONStream).)*$/'",
    "buildSets": "nf run node runner.js buildSets",
    "constants": "nf run node runner.js constants",
    "webpack": "webpack -p",
    "watch": "webpack --watch",
    "maven": "mvn -q -f parser/pom.xml clean install -U",
    "build": "npm install && npm run maven && npm run webpack && npm run constants",
    "yasp-parser": "sudo nf export -o /etc/init/ parser && sudo restart foreman",
    "yasp-core": "sudo nf export -o /etc/init/ web,worker,parser,parseManager,scanner,skill && sudo restart foreman"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/yasp-dota/yasp"
  },
  "dependencies": {
    "JSONStream": "0.10.0",
    "async": "^1.4.0",
    "big-number": "0.3.1",
    "body-parser": "1.13.2",
    "bootstrap": "^3.3.5",
    "c3": "^0.4.10",
    "cal-heatmap": "^3.5.2",
    "compression": "^1.5.1",
    "connect-redis": "^2.4.0",
    "cookie-session": "^1.2.0",
    "d3": "^3.5.6",
    "datatables": "^1.10.7",
    "datatables-bootstrap3-plugin": "^0.4.0",
    "dota2": "yasp-dota/node-dota2",
    "dota2-minimap-hero-sprites": "^1.0.1",
    "dotenv": "~1.2.0",
    "express": "~4.13.1",
    "express-session": "~1.11.3",
    "font-awesome": "^4.3.0",
    "foreman": "~1.4.1",
    "heatmap.js": "^2.0.1",
    "http-auth": "~2.2.8",
    "http-proxy": "^1.11.1",
    "jade": "~1.11.0",
    "jquery": "^2.1.4",
    "kue": "~0.9.4",
    "moment": "^2.10.3",
    "monk": "1.0.1",
    "numeral": "^1.5.3",
    "parse-redis-url": "0.0.2",
    "passport": "0.2.2",
    "passport-steam": "0.1.5",
    "paypal-rest-sdk": "~1.6",
    "poet": "~2.0.1",
    "qTip2": "yasp-dota/qtip2",
    "redis": "0.12.1",
    "request": "^2.59.0",
    "request-progress": "^0.3.1",
    "select2": "^4.0.0",
    "select2-bootstrap-theme": "select2/select2-bootstrap-theme",
    "sentiment": "^1.0.2",
    "socket.io": "^1.3.6",
    "socket.io-client": "^1.3.6",
    "steam": "0.6.8",
    "sticky-session": "^0.1.0",
    "winston": "1.0.1",
    "wordcloud": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^5.7.4",
    "babel-eslint": "^3.1.23",
    "babel-loader": "^5.3.2",
    "coveralls": "^2.11.3",
    "css-loader": "^0.15.5",
    "database-cleaner": "~0.9.2",
    "eslint": "^0.24.1",
    "eslint-config-airbnb": "0.0.6",
    "file-loader": "^0.8.4",
    "istanbul": "~0.3.17",
    "json-loader": "^0.5.2",
    "mocha": "~2.2.5",
    "mongodb": "~2.0.39",
    "nock": "^2.9.0",
    "nodemon": "^1.3.8",
    "style-loader": "^0.12.3",
    "supertest": "^1.0.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.1"
  },
  "engines": {
    "node": ">= 0.12.0"
  }
}
