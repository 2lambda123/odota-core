{
  "name": "yasp",
  "description": "Yet Another (Dota) Stats Page",
  "version": "1.2.9",
  "scripts": {
    "start": "nf start",
    "test": "mocha",
    "travistest": "istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | codeclimate && rm -rf ./coverage",
    "unparsed": "grunt unparsed",
    "constants": "grunt constants",
    "fullhistory": "grunt fullhistory",
    "unnamed": "grunt unnamed",
    "postinstall": "mvn -q -f parser/pom.xml package && npm run constants"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/yasp-dota/yasp"
  },
  "dependencies": {
    "async": "0.9.0",
    "aws-sdk": "~2.1.7",
    "big-number": "0.3.1",
    "body-parser": "1.10.2",
    "cheerio": "0.18.0",
    "cookie-session": "~1.1.0",
    "express": "~4.11.1",
    "express-session": "~1.10.1",
    "http-auth": "~2.2.5",
    "jade": "~1.9.1",
    "kue": "~0.8.11",
    "moment": "2.9.0",
    "monk": "0.9.1",
    "passport": "0.2.1",
    "passport-steam": "0.1.4",
    "redis": "0.12.1",
    "request": "~2.51.0",
    "winston": "0.8.3",
    "multer": "~0.1.7",
    "foreman": "~1.2.1",
    "dotenv": "~0.4.0",
    "recaptcha": "1.2.0",
    "parse-redis-url": "0.0.1",
    "dota2": "~0.6.2",
    "steam": "~0.6.7",
    "compressjs": "~1.0.2",
    "streamifier": "~0.1.0"
  },
  "devDependencies": {
    "grunt-cli": "~0.1.13",
    "grunt": "~0.4.5",
    "supertest": "~0.15.0",
    "mocha": "~2.1.0",
    "istanbul": "~0.3.5",
    "database-cleaner": "~0.9.0",
    "mongodb": "~1.4.29",
    "codeclimate-test-reporter": "0.0.4",
    "nock": "~0.57.0"
  }
}
