{
  "name": "opendota-core",
  "description": "Open source Dota data platform",
  "version": "22.0.0",
  "license": "MIT",
  "scripts": {
    "start": "tsx index.mts",
    "test": "NODE_ENV=test mocha --exit",
    "build": "npm install && npm run typecheck",
    "tsc": "tsc --project tsconfig.json",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint . --ext .js,.mjs --ignore-path .gitignore",
    "lintfix": "eslint . --ext .js,.mjs --ignore-path .gitignore --fix",
    "rediskeys": "redis-cli keys '*' | cut -d':' -f1 | sort | uniq -c",
    "resetpicks": "redis-cli keys 'picks_*' | xargs redis-cli del",
    "resetrecords": "redis-cli keys 'records*' | xargs redis-cli del",
    "dockerbuild": "sudo docker build -t odota/core .",
    "psql": "docker exec -it odota-postgres psql -U postgres -d yasp",
    "prettier": "prettier --write ."
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/odota/core"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.462.0",
    "@elastic/elasticsearch": "^7.17.0",
    "async": "^2.6.1",
    "axios": "^1.6.2",
    "body-parser": "^1.19.0",
    "cassandra-driver": "^4.4.0",
    "compression": "^1.7.4",
    "cookie-session": "2.0.0-beta.3",
    "cors": "^2.8.5",
    "dota2": "github:odota/node-dota2",
    "dotaconstants": "^7.18.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-proxy": "^1.18.1",
    "ioredis": "^5.3.2",
    "json-bigint": "^0.3.0",
    "JSONStream": "^1.3.5",
    "knex": "^0.21.12",
    "long": "^4.0.0",
    "moment": "^2.24.0",
    "nock": "^13.0.5",
    "passport": "^0.4.1",
    "passport-steam": "^1.0.18",
    "pg": "^8.2.1",
    "pm2": "^5.3.0",
    "request": "^2.88.2",
    "simple-vdf": "^1.1.1",
    "steam": "github:odota/node-steam-npm",
    "steam-resources": "github:odota/node-steam-resources",
    "stripe": "^9.12.0",
    "supertest": "^4.0.2",
    "tsx": "^4.6.2",
    "uuid": "^3.3.3",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.0",
    "mocha": "^8.2.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  }
}
