{
  "extends": ["airbnb/base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "max-len": [
      "error",
      {
        "code": 180,
        "ignoreTemplateLiterals": true,
        "ignoreStrings": true
      }
    ],
    "no-console": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "arrow-body-style": 0,
    "quotes": ["error", "double"]
  }
}
