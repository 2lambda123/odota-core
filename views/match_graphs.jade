extend match

append head
  script.
    var data = !{JSON.stringify(match.graphData)}
    
append content 
    .row
        if match.parsed_data
          .row
            .col-md-12
              h3 Radiant Advantage
              #chart-diff
          .row
            include ads/leaderboard_2
          .row
            .col-md-12
              h3 Gold
              #chart-gold
          .row
            include ads/leaderboard_3
          .row
            .col-md-12
              h3 XP
              #chart-xp
            .col-md-12
              h3 LH
              #chart-lh
        else
          h1 This match has no parsed data!

append footer_assets
  //- C3 assets
  link(rel='stylesheet', href='//cdn.rawgit.com/masayuki0812/c3/master/c3.css')
  script(src='//cdnjs.cloudflare.com/ajax/libs/async/0.9.0/async.js')
  script(src='//cdn.jsdelivr.net/d3js/latest/d3.min.js')
  script(src='//cdn.rawgit.com/masayuki0812/c3/master/c3.js')
  script
    include ../public/js/chart.js
  script
    include ../public/js/yasp.js
